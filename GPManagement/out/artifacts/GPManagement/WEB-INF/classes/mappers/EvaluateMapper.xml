<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.ylxt.dao.IEvaluateMapper">
    <resultMap id="BaseResultMap" type="com.ylxt.pojo.Evaluate"/>

    <insert id="submitEvaluate" parameterType="com.ylxt.pojo.Evaluate">
        INSERT INTO evaluate_table VALUES
        (null, #{number}, #{studentName}, #{subjectName}, #{subjectId}, #{attachment}, #{guideTeacher}, #{evaluate}, #{score})
    </insert>


</mapper>
